    "zustand": "^3.5.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:firestore": "react-scripts test --detectOpenHandles --verbose ./src/tests/firestore",
    "emulator": "firebase emulators:start",
    "firestore:rules:dev": "firebase use dev && firebase deploy --only firestore:rules",
    "firestore:rules:prod": "firebase use prod && firebase deploy --only firestore:rules",
    "firestore:indexes:dev": "firebase use dev && firebase deploy --only firestore:indexes",
    "firestore:indexes:prod": "firebase use prod && firebase deploy --only firestore:indexes",
    "storage:dev": "firebase use dev && firebase deploy --only storage",
    "storage:prod": "firebase use prod && firebase deploy --only storage",
    "functions:dev": "firebase use dev && firebase deploy --only functions",
    "functions:prod": "firebase use prod && firebase deploy --only functions",
    "build:dev": "env-cmd -f .env.dev react-scripts build",
    "build:prod": "env-cmd -f .env.prod react-scripts build",
    "hosting:dev": "env-cmd -f .env.dev npm run build && firebase deploy --only hosting:dashboard -P dev",
    "hosting:prod": "env-cmd -f .env.prod npm run build && firebase deploy --only hosting:dashboard -P prod"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@firebase/testing": "^0.20.11",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.6",
    "@types/react": "^16.14.14",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-swipeable-views": "^0.13.0",
    "@types/react-table": "^7.7.1",
    "env-cmd": "^10.1.0",
    "jest": "^26.6.0"
  }
}
