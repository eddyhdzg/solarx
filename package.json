{
  "name": "solarx-dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@hookform/resolvers": "^2.8.2",
    "@mui/icons-material": "^5.0.5",
    "@mui/lab": "^5.0.0-alpha.53",
    "@mui/material": "^5.0.6",
    "@mui/styles": "^5.0.2",
    "dayjs": "^1.10.7",
    "firebase": "^9.2.0",
    "i18next": "^21.3.3",
    "i18next-browser-languagedetector": "^6.1.2",
    "immer": "^9.0.6",
    "match-sorter": "^6.3.1",
    "node-sass": "^6.0.1",
    "notistack": "^2.0.2",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.4.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.18.0",
    "react-i18next": "^11.13.0",
    "react-notion": "^0.10.0",
    "react-number-format": "^4.7.3",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "react-swipeable-views": "^0.14.0",
    "react-table": "^7.7.0",
    "react-use-clipboard": "^1.0.7",
    "reactfire": "^4.2.0",
    "recharts": "^2.1.6",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "web-vitals": "^2.1.2",
    "workbox-core": "^6.3.0",
    "workbox-expiration": "^6.3.0",
    "workbox-routing": "^6.3.0",
    "workbox-strategies": "^6.3.0",
    "yup": "^0.32.11",
    "zustand": "^3.6.2"
  },
  "scripts": {
    "========== LOCAL ==========": "",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:firestore": "react-scripts test  --runInBand --detectOpenHandles --verbose ./src/tests/firestore",
    "emulator": "firebase --project='solarx-dev' emulators:start",
    "kill": "lsof -ti tcp:8080 | xargs kill",
    "========== DEV ==========": "",
    "firestore:rules:dev": "firebase deploy --project solarx-dev --only firestore:rules",
    "firestore:indexes:dev": "firebase deploy --project solarx-dev --only firestore:indexes",
    "storage:dev": "firebase deploy --project solarx-dev --only storage",
    "functions:dev": "firebase deploy --project solarx-dev --only functions",
    "========== CI ==========": "",
    "build:dev": "env-cmd -f .env.dev react-scripts build",
    "build:prod": "env-cmd -f .env.prod react-scripts build",
    "hosting:dev": "env-cmd -f .env.dev npm run build && firebase deploy --only hosting:dashboard -P dev",
    "hosting:prod": "env-cmd -f .env.prod npm run build && firebase deploy --only hosting:dashboard -P prod",
    "========== PROD ==========": "",
    "firestore:rules:prod": "firebase use prod && firebase deploy --only firestore:rules",
    "firestore:indexes:prod": "firebase use prod && firebase deploy --only firestore:indexes",
    "storage:prod": "firebase use prod && firebase deploy --only storage",
    "functions:prod": "firebase use prod && firebase deploy --only functions"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@firebase/testing": "^0.20.11",
    "@testing-library/jest-dom": "^5.14.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dom": "^17.0.10",
    "@types/react-helmet": "^6.1.4",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-swipeable-views": "^0.13.1",
    "@types/react-table": "^7.7.7",
    "env-cmd": "^10.1.0"
  },
  "permissions": [
    "notifications",
    "activeTab"
  ]
}
