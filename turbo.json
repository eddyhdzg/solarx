{
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ]
    },
    "serve": {},
    "lint": {},
    "dev": {
      "cache": false
    },
    "storybook": {
      "cache": false
    },
    "build-storybook": {
      "outputs": [
        "storybook-static/**"
      ]
    }
  }
}
